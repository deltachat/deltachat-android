    <!--Intro view-->
    <string name="Intro1Headline">Delta Chat</string>
    <string name="Intro1Message">The messenger with the <![CDATA[<b>broadest reach</b>]]> worldwide. <![CDATA[<b>Free</b>]]> and <![CDATA[<b>independent</b>]]>.</string>

    <string name="Intro2Headline">Independent</string>
    <string name="Intro2Message"><![CDATA[<b>There is no Delta Chat server.</b>]]> The app only uses the mail server of your email address.</string>

    <string name="Intro3Headline">Fast</string>
    <string name="Intro3Message"><![CDATA[<b>Delivery</b>]]> in seconds.<![CDATA[<br/>]]>Rapid interface.</string>

    <string name="Intro4Headline">Powerful</string>
    <string name="Intro4Message"><![CDATA[<b>Unlimited</b>]]> chats, images, videos, audio messages and more. Use one account on many devices.</string>

    <string name="Intro5Headline">Free</string>
    <string name="Intro5Message"><![CDATA[<b>Delta Chat</b>]]> is copylefted, forever libre.<![CDATA[<br/>]]>No ads. No subscription. No cost. No vendor lock-in.</string>

    <string name="Intro6Headline">Encrypted</string>
    <string name="Intro6Message"><![CDATA[<b>Encrypted</b>]]> with all common algorithms. Messages behave like unprotected email, but are secure.</string>

    <string name="Intro7Headline">Trustworthy</string>

