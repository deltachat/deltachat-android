# EditorConfig (https://editorconfig.org/)

root = true

[*]
end_of_line = lf
charset = utf-8
insert_final_newline = true

[*.java]
indent_style = space
indent_size = 2

[*.c]
indent_style = space
indent_size = 4

[*.gradle]
indent_style = space
indent_size = 4

[*.sh]
indent_style = space
indent_size = 4
